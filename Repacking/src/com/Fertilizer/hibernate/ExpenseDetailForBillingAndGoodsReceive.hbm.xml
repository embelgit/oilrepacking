<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 

    <hibernate-mapping>
    
    <class name="com.Fertilizer.hibernate.ExpenseDetailForBillingAndGoodsReceiveBean" 
    table="expense_details_for_billing_and_goods_receive" schema="oil"
    >
    
    	<id name="pkExpenseForBillingId" type="java.lang.Long" column="pk_expense_for_billing">
			<generator class="increment" />
		</id>


		<property 
		name="expenseName" 
		type="java.lang.String" 
		column="expense_name"
		length="128" />
		
		 <property
        name="insertDate"
        type="java.sql.Date"
        column="insert_date"
       
    	/>
			
		</class>
    </hibernate-mapping>